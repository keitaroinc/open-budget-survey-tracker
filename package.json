{
  "name": "open-budget-survey-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "node_modules/mocha/bin/mocha test --recursive",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- test/*.js --recursive --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "watch": "gulp watch",
    "postinstall": "bower install && gulp build && gulp deploy"
  },
  "engines": {
    "node": "0.10.x"
  },
  "dependencies": {
    "body-parser": "~1.0.0",
    "bower": "1.3.8",
    "cookie-parser": "~1.0.1",
    "debug": "~0.7.4",
    "del": "0.1.1",
    "express": "~4.2.0",
    "gulp": "3.8.7",
    "gulp-clean": "0.3.1",
    "gulp-concat": "2.3.4",
    "gulp-extend": "0.2.0",
    "gulp-imagemin": "3.0.3",
    "gulp-less": "1.3.3",
    "gulp-minify-css": "0.3.7",
    "gulp-rev": "1.0.0",
    "gulp-sourcemaps": "1.1.1",
    "gulp-uglify": "0.3.1",
    "i18n-abide": "0.0.22",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-pngquant": "^5.0.0",
    "moment": "2.8.1",
    "morgan": "~1.0.0",
    "open-budget-indaba-client": "git+https://github.com/keitaroinc/open-budget-indaba-client.git#b60830eebbee13cdcc8f6998c43d0c9857b616a7",
    "request": "~2.39.0",
    "swig": "~1.4.2",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.4",
    "mocha": "~1.21.0",
    "sinon": "~1.7.3"
  }
}
